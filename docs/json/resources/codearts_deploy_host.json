{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_codearts_deploy_host": {
          "block": {
            "attributes": {
              "as_proxy": {
                "type": "bool",
                "description": "Specifies whether the host is an agent host.",
                "optional": true,
                "computed": true,
                "forcenew": true
              },
              "connection_status": {
                "type": "string",
                "description": "The connection status.",
                "computed": true
              },
              "created_at": {
                "type": "string",
                "description": "The create time.",
                "computed": true
              },
              "group_id": {
                "type": "string",
                "description": "Specifies the CodeArts deploy group ID.",
                "required": true,
                "forcenew": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "install_icagent": {
                "type": "bool",
                "description": "Specifies whether to enable Application Operations Management (AOM) for free to provide\nmetric monitoring, log query and alarm functions.",
                "optional": true
              },
              "ip_address": {
                "type": "string",
                "description": "Specifies the IP address.",
                "required": true
              },
              "lastest_connection_at": {
                "type": "string",
                "description": "The last connection time.",
                "computed": true
              },
              "name": {
                "type": "string",
                "description": "Specifies the host name.",
                "required": true
              },
              "os_type": {
                "type": "string",
                "description": "Specifies the operating system.",
                "required": true,
                "forcenew": true
              },
              "password": {
                "type": "string",
                "description": "Specifies the password.",
                "optional": true,
                "sensitive": true
              },
              "permission": {
                "type": [
                  "list",
                  [
                    "object",
                    {
                      "can_add_host": "bool",
                      "can_copy": "bool",
                      "can_delete": "bool",
                      "can_edit": "bool",
                      "can_view": "bool"
                    }
                  ]
                ],
                "computed": true
              },
              "port": {
                "type": "number",
                "description": "Specifies the SSH port.",
                "required": true
              },
              "private_key": {
                "type": "string",
                "description": "Specifies the private key.",
                "optional": true,
                "sensitive": true
              },
              "proxy_host_id": {
                "type": "string",
                "description": "Specifies the agent ID.",
                "optional": true,
                "computed": true
              },
              "region": {
                "type": "string",
                "computed": true
              },
              "sync": {
                "type": "bool",
                "description": "Specifies whether to synchronize the password of the current host to the hosts with the\nsame IP address, username and port number in other group in the same project.",
                "optional": true
              },
              "updated_at": {
                "type": "string",
                "description": "The update time.",
                "computed": true
              },
              "username": {
                "type": "string",
                "description": "Specifies the username.",
                "required": true
              }
            }
          },
          "product": {
            "short": "CodeArtsDeploy",
            "name": "部署",
            "catalog": "CodeArts",
            "name_cn": "部署",
            "catalog_cn": "软件开发生产线"
          }
        }
      }
    }
  }
}
