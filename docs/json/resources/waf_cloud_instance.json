                "forcenew": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "period": {
                "type": "number",
                "optional": true,
                "forcenew": true
              },
              "period_unit": {
                "type": "string",
                "optional": true,
                "forcenew": true
              },
              "region": {
                "type": "string",
                "description": "Specifies the region where the cloud WAF is located.",
                "computed": true
              },
              "resource_spec_code": {
                "type": "string",
                "description": "Specifies the specification of the cloud WAF.",
                "optional": true
              },
              "status": {
                "type": "number",
                "description": "The current status of the cloud WAF.",
                "computed": true
              },
              "website": {
                "type": "string",
                "description": "Specifies the website to which the account belongs.",
                "optional": true
              }
            },
            "block_types": {
              "bandwidth_expack_product": {
                "block": {
                  "attributes": {
                    "resource_size": {
